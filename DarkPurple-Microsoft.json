{
  
  "workbench.colorTheme": "Visual Studio Dark - C++",
  
  "workbench.colorCustomizations": {
    "statusBar.background": "#5C2E91",
    "activityBar.background": "#4E257F",
    "sideBar.background": "#3A2066",
    "list.hoverBackground": "#6F42C1",
    "list.activeSelectionBackground": "#5C2E91",
    "list.inactiveSelectionBackground": "#46307A",
    "list.focusOutline": "#6F42C1",
    "titleBar.activeBackground": "#3A2066",
    "titleBar.inactiveBackground": "#3A2066",
    "scrollbarSlider.background": "#5C2E9140",
    "scrollbarSlider.hoverBackground": "#5C2E9160",
    "scrollbarSlider.activeBackground": "#5C2E9180",
    "editor.background": "#2A183A",
    "editorLineNumber.activeForeground": "#CDBFE6",
    "editorLineNumber.foreground": "#9575CD",
    "editor.selectionBackground": "#4A288C",
    "tab.activeBackground": "#2A183A",
    "tab.inactiveBackground": "#3A2066",
    "tab.inactiveForeground": "#9575CD",
    "editorGroupHeader.tabsBackground": "#3A2066"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "keyword",
          "keyword.operator",
          "keyword.control"
        ],
        "settings": {
          "foreground": "#FF5370"
        }
      },
      {
        "scope": [
          "variable",
          "variable.function",
          "entity.name.function"
        ],
        "settings": {
          "foreground": "#B39DDB"
        }
      },
      {
        "scope": [
          "string",
          "constant.numeric",
          "constant.character.escape"
        ],
        "settings": {
          "foreground": "#FFCC80"
        }
      },
      {
        "scope": [
          "comment",
          "comment.block.documentation"
        ],
        "settings": {
          "foreground": "#B2ABBF"
        }
      },
      {
        "scope": [
          "entity.name.tag",
          "meta.tag"
        ],
        "settings": {
          "foreground": "#FFCB6B"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#82AAFF"
        }
      },
      {
        "scope": [
          "string.quoted.double.html"
        ],
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "scope": [
          "entity.name.selector",
          "entity.name.selector.class",
          "entity.name.selector.id"
        ],
        "settings": {
          "foreground": "#F07178"
        }
      },
      {
        "scope": [
          "support.type.property-name"
        ],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "scope": [
          "support.constant.property-value"
        ],
        "settings": {
          "foreground": "#FFCC80"
        }
      },
      {
        "scope": [
          "string.quoted.double.json"
        ],
        "settings": {
          "foreground": "#C792EA"
        }
      },
      {
        "scope": [
          "support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#82AAFF"
        }
      },
      {
        "scope": [
          "entity.name.tag.css",
          "entity.name.tag.less",
          "entity.name.tag.custom.scss"
        ],
        "settings": {
          "foreground": "#F07178"
        }
      },
      {
        "scope": [
          "support.type.property-name.css",
          "support.type.property-name.less",
          "support.type.property-name.scss"
        ],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "scope": [
          "support.constant.property-value.css",
          "support.constant.property-value.less",
          "support.constant.property-value.scss",
          "variable.other.constant.property-value.css",
          "variable.other.constant.property-value.less",
          "variable.other.constant.property-value.scss"
        ],
        "settings": {
          "foreground": "#FFCC80"
        }
      }
    ]
  }
}